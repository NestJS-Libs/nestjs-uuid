{
  "name": "nestjs-uuid",
  "version": "0.0.6",
  "description": "A NestJS module for generating UUIDs",
  "author": "Fernando Dorantes <fernando@dorant.es>",
  "private": false,
  "license": "MIT",
  "main": "dist/index.js",
  "url": "https://github.com/NestJS-Libs/nestjs-uuid",
  "repository": {
    "type": "git",
    "url": "https://github.com/NestJS-Libs/nestjs-uuid"
  },
  "keywords": [
    "nest",
    "nestjs",
    "uuid",
    "uuidv1",
    "uuidv4"
  ],
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
